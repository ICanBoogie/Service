{
	"name": "icanboogie/service",
	"type": "library",
	"description": "A dependency-injection container agnostic service provider",
	"keywords": [ "dependency", "injection", "provider" ],
	"homepage": "https://icanboogie.org/",
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Olivier Laviale",
			"email": "olivier.laviale@gmail.com",
			"homepage": "https://olvlvl.com/",
			"role" : "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/ICanBoogie/Service/issues",
		"source": "https://github.com/ICanBoogie/Service"
	},
	"prefer-stable": true,
	"prefer-dist": true,
	"require": {
		"php": ">=7.2"
	},
	"require-dev": {
		"phpunit/phpunit": "^8.5"
	},
	"autoload": {
		"psr-4": {
			"ICanBoogie\\Service\\": "lib/"
		},
		"files": [ "helpers.php" ]
